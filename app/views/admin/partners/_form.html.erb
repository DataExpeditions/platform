<%= simple_form_for [:admin, @partner], wrapper: :vertical_input_group do |f| %>
  <%= f.input :name %>
  <%= f.input :description, as: :text %>
  <%= f.input :website_url%>
  <%= f.input :logo, required: !f.object.logo? %>
  <% if f.object.logo? %>
    <%= image_tag f.object.logo.normal.url %>
  <% end %>
  <%= f.submit %>
<% end %>
